extends layout

block content
  h1.
    Photos
  div.row
    each photo, i in photos
      div.col-xs-12.col-sm-4.col-lg-2.photo-box
        img(src="#{photo.url}")
        form(name="deletephoto",method="post",action="/admin/deletephoto")
          input(type="hidden",name="id",value="#{photo.id}")
          button(type="submit") delete
